{
  "preset": "google",
  "maximumLineLength": {
    "value": 120,
    "allExcept": ["comments", "regex"]
  },
  "disallowSpacesInAnonymousFunctionExpression": null,
  "validateLineBreaks": "LF",
  "validateIndentation": 2,
  "validateQuoteMarks": "'",
  "requireDotNotation": false,
  "requireCamelCaseOrUpperCaseIdentifiers": true,
  "esnext": true
}
